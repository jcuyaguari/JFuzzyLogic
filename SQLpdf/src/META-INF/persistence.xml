    <class>testing.Medico</class>
    <class>testing.Productos</class>
    <class>testing.Receta</class>
    <class>testing.Recetadetalle</class>
    <class>testing.Hospital</class>
    <class>miprac.P1producto</class>
    <class>miprac.Fproducto</class>
    <class>miprac.P1proveedores</class>
    <class>miprac.P2producto</class>
    <class>miprac.Fproveedores</class>
    <class>miprac.P2proveedores</class>
    <class>examen.Carro</class>
    <class>examen.Producto</class>
    <class>examen.Detalle</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/receta?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value=""/>
